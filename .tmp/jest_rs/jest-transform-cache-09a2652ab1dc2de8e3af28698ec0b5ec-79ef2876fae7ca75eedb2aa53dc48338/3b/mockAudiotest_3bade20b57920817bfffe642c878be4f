var _chords = require("../../src/constants/chords");
var _mockAudio = require("../../src/utils/mockAudio");
describe('getMockDetectedChord', function () {
  it('returns a deterministic chord from the list', function () {
    var first = (0, _mockAudio.getMockDetectedChord)(0, 123);
    var second = (0, _mockAudio.getMockDetectedChord)(_chords.chords.length, 456);
    expect(first.name).toBe(_chords.chords[0].primaryName);
    expect(first.notes).toEqual(_chords.chords[0].notes);
    expect(first.timestamp).toBe(123);
    expect(second.name).toBe(_chords.chords[0].primaryName);
    expect(second.timestamp).toBe(456);
  });
});
