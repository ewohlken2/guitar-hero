var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _ChordDiagram = require("../../src/components/ChordDiagram");
var _chords = require("../../src/constants/chords");
var _jsxRuntime = require("react/jsx-runtime");
describe('ChordDiagram', function () {
  var gMajor = _chords.chords.find(function (c) {
    return c.id === 'g-major';
  });
  it('renders chord name', function () {
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ChordDiagram.ChordDiagram, {
      chord: gMajor
    }));
    expect(_reactNative.screen.getByText('G')).toBeTruthy();
  });
  it('renders with custom size', function () {
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ChordDiagram.ChordDiagram, {
      chord: gMajor,
      size: "large"
    }));
    expect(_reactNative.screen.getByText('G')).toBeTruthy();
  });
});
