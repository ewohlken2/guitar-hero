var _audioDetection = require("../../modules/audio-detection");
describe('audio detection module bridge', function () {
  it('exposes start/stop functions', function () {
    expect(typeof _audioDetection.startListening).toBe('function');
    expect(typeof _audioDetection.stopListening).toBe('function');
    expect(typeof _audioDetection.addListener).toBe('function');
  });
});
